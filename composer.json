    "type": "library",
    "keywords": [
        "php",
        "command-bus"
    ],
    "version": "1.1.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Sasa Blagojevic",
            "email": "sasa.blagojevic@mail.com"
        }
